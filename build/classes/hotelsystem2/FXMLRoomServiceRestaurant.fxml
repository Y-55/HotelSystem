<VBox alignment="TOP_CENTER" prefHeight="700.0" prefWidth="950.0" spacing="10.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotelsystem2.FXMLRoomServiceRestaurantController">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="73.0" prefWidth="890.0">
