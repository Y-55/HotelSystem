            <Label text="Room Service" textFill="#ff5e36">
               <font>
                  <Font name="System Bold" size="32.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
         <children>
            <JFXComboBox prefHeight="31.0" prefWidth="185.0" promptText="Service Type" />
            <JFXTextField prefHeight="31.0" prefWidth="186.0" promptText="Phone" text="Description" />
